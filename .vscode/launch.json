    "version": "0.2.0",
    "configurations": [
        {
            "name": "Launch Dashboard",
            "request": "launch",
            "type": "chrome",
            "url": "${workspaceFolder}/index.html",
            "webRoot": "${workspaceFolder}"
        },
        {
            "name" : "Launch Chrome",
            "type" : "chrome",
            "request" : "launch",
            "url" : "http://localhost:5500/index.html",
            "webRoot" : "${workspaceFolder}"
        }
        {
            "name": "Start ExcelParser",
            "type": "python",
            "request": "launch",
            "program": "${workspaceFolder}/py/excelParser.py",
            "console": "integratedTerminal",
            "justMyCode": true
        },
        {
            "name": "Start Server",
            "type": "python",
            "request": "launch",
            "program": "${workspaceFolder}/py/server.py",
            "args": ["2000"],
            "console": "integratedTerminal",
            "justMyCode": true
        },
        {
            "name": "Start js file",
            "type": "node",
            "request": "launch",
            "program": "${workspaceFolder}/js/sortTesting.js",
            "console": "integratedTerminal"       
        },
        {
            "name": "Start py script",
            "type": "python",
            "request": "launch",
            "program": "${workspaceFolder}/py/data_merger.py",
            "console": "integratedTerminal",
            "justMyCode": true
        },

    ]
}